    "i18next": "19.1.0",
    "i18next-express-middleware": "1.9.1",
    "i18next-sync-fs-backend": "1.1.1",
    "i18next-xhr-backend": "3.2.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-i18next": "11.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
